{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "ModularMonolith": {
        "ClusterId": "ModuleCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "ModuleCluster": {
        "Destinations": {
          "base": {
            "Address": "https://localhost:7112"
          }
        }
      }
    }
  }
}